{
  "name": "Iris",
  "description": "A tool made to check any Hypixel player's last session easily and quickly!",
  "version": "1.1.8",
  "manifest_version": 3,
  "background": {
    "service_worker": "./src/background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "./src/player/player.html",
    "default_icon": {
      "16": "./images/iris16.png",
      "32": "./images/iris32.png",
      "48": "./images/iris48.png",
      "128": "./images/iris128.png"
    }
  },
  "icons": {
    "16": "./images/iris16.png",
    "32": "./images/iris32.png",
    "48": "./images/iris48.png",
    "128": "./images/iris128.png"
  }
}
